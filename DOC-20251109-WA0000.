    document.addEventListener('DOMContentLoaded', function () {
        // 'menuToggle' ID ‡§π‡•à‡§Æ‡§¨‡§∞‡•ç‡§ó‡§∞ ‡§Ü‡§á‡§ï‡§® (‡§§‡•Ä‡§® ‡§≤‡§æ‡§á‡§®) ‡§ï‡§æ ‡§π‡•à‡•§
        const menuToggle = document.getElementById('menuToggle');
        // 'mainNav' ID ‡§Æ‡•á‡§®‡•Ç ‡§≤‡§ø‡§∏‡•ç‡§ü (Home, Designs, Services, Contact) ‡§ï‡§æ ‡§π‡•à‡•§
        const mainNav = document.getElementById('mainNav');
        // ‡§∏‡§≠‡•Ä ‡§≤‡§ø‡§Ç‡§ï‡•ç‡§∏ ‡§ï‡•ã ‡§∏‡•á‡§≤‡•á‡§ï‡•ç‡§ü ‡§ï‡§∞‡§®‡§æ ‡§§‡§æ‡§ï‡§ø ‡§ï‡•ç‡§≤‡§ø‡§ï ‡§ï‡§∞‡§®‡•á ‡§™‡§∞ ‡§Æ‡•á‡§®‡•Ç ‡§¨‡§Ç‡§¶ ‡§π‡•ã ‡§ú‡§æ‡§è‡•§
        const navLinks = mainNav.querySelectorAll('a');

        // ‡§ú‡§¨ ‡§π‡•à‡§Æ‡§¨‡§∞‡•ç‡§ó‡§∞ ‡§Ü‡§á‡§ï‡§® ‡§™‡§∞ ‡§ï‡•ç‡§≤‡§ø‡§ï ‡§ï‡§ø‡§Ø‡§æ ‡§ú‡§æ‡§§‡§æ ‡§π‡•à
        menuToggle.addEventListener('click', function () {
            // 'open' ‡§ï‡•ç‡§≤‡§æ‡§∏ ‡§ï‡•ã ‡§ú‡•ã‡§°‡§º‡§§‡§æ ‡§Ø‡§æ ‡§π‡§ü‡§æ‡§§‡§æ ‡§π‡•à, ‡§ú‡§ø‡§∏‡§∏‡•á CSS ‡§Æ‡•á‡§®‡•Ç ‡§ï‡•ã ‡§¶‡§ø‡§ñ‡§æ‡§§‡§æ ‡§π‡•à
            mainNav.classList.toggle('open');
        });

        // ‡§ú‡§¨ ‡§ï‡§ø‡§∏‡•Ä ‡§Æ‡•á‡§®‡•Ç ‡§≤‡§ø‡§Ç‡§ï (Home, Designs, ‡§Ü‡§¶‡§ø) ‡§™‡§∞ ‡§ï‡•ç‡§≤‡§ø‡§ï ‡§ï‡§ø‡§Ø‡§æ ‡§ú‡§æ‡§§‡§æ ‡§π‡•à
        navLinks.forEach(link => {
            link.addEventListener('click', function () {
                // ‡§õ‡•ã‡§ü‡•á ‡§∏‡•ç‡§ï‡•ç‡§∞‡•Ä‡§® ‡§™‡§∞, ‡§≤‡§ø‡§Ç‡§ï ‡§™‡§∞ ‡§ï‡•ç‡§≤‡§ø‡§ï ‡§ï‡§∞‡§®‡•á ‡§ï‡•á ‡§¨‡§æ‡§¶ ‡§Æ‡•á‡§®‡•Ç ‡§ï‡•ã ‡§¨‡§Ç‡§¶ ‡§ï‡§∞ ‡§¶‡•á‡§Ç‡•§
                if (window.innerWidth < 768) {
                    mainNav.classList.remove('open');
                }
                
                // ‡§∏‡•ç‡§Æ‡•Ç‡§• ‡§∏‡•ç‡§ï‡•ç‡§∞‡•â‡§≤‡§ø‡§Ç‡§ó ‡§ï‡•á ‡§≤‡§ø‡§è (‡§Ø‡§¶‡§ø ‡§¨‡•ç‡§∞‡§æ‡§â‡§ú‡§º‡§∞ ‡§∏‡§™‡•ã‡§∞‡•ç‡§ü ‡§ï‡§∞‡§§‡§æ ‡§π‡•à)
                const targetId = this.getAttribute('href').substring(1);
                const targetSection = document.getElementById(targetId);
                if (targetSection) {
                    targetSection.scrollIntoView({ behavior: 'smooth' });
                }
            });
        });
    });
</script>